<IfModule mod_rewrite.c>
	
	RewriteEngine on
	
#	RewriteCond %{DOCUMENT_ROOT} !-f

	RewriteBase /smarty/
	
	RewriteCond %{REQUEST_FILENAME} \.(js|ico|gif|jpg|png|css|html|swf|flv|xml)$ 
	RewriteRule ^.*$ - [L] 	
    
    RewriteRule	^$	www/	[L]
    RewriteRule	(.*) www/$1	[L]
	
</IfModule>